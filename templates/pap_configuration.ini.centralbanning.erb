[paps]
centralbanning.type = remote
centralbanning.enabled = <%=@centralbanning_enabled%>
centralbanning.dn = <%=@centralbanning_dn%>
centralbanning.hostname = <%=@centralbanning_hostname%>
centralbanning.port = <%=@centralbanning_port%>
centralbanning.path = /pap/services/
centralbanning.protocol = https
centralbanning.public = false

[paps:properties]
poll_interval = <%=@poll_interval%>
ordering = centralbanning
ordering = default

